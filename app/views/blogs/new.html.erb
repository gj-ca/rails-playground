<h1>Blog#new</h1>
 
<%= form_with model: @blog, local: true do |form| %>

    <div class="field">
        <%= form.label :title %>
        <%= form.text_field :title%>
    </div>
    <div class="field">
        <%= form.label :body %>
        <%= form.text_field :body%>
    </div>

    <div class="field">
        <%= form.label :url %>
        <%= form.text_field :url %>
    </div>

    <div class="actions">
        <%= form.submit %>
    </div>
<% end %>

